{
  "expo": {
    "name": "TechLand Marketplace",
    "slug": "marketplacesergej",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0c14"
    },
