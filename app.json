    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f0c14"
      },
      "package": "com.sergejtoropov.marketplacesergej"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
